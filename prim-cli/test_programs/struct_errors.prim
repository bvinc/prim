struct Point {
    x: i32,
    y: i32
}

fn main() {
    // This should cause a type error - missing field 'y'
    let p1 = Point { x = 10 }
    
    // This should cause a type error - extra field 'z'
    let p2 = Point { x = 5, y = 15, z = 25 }
    
    // This should cause a type error - field type mismatch
    let p3 = Point { x = "hello", y = 20 }
    
    // This should cause a type error - unknown struct
    let p4 = UnknownStruct { field = 42 }
    
    // This should cause a type error - field access on non-struct
    let x = 42
    let y = x.field
}