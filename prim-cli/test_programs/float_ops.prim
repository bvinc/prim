import std.io.println_bool
import std.io.println_f32
import std.io.println_f64

fn main() {
    println_f32(3.5f32);
    println_f64(1.25f64);

    let sum_f32: f32 = 1.5f32 + 2.25f32;
    println_f32(sum_f32);

    let div_f64: f64 = 7.5f64 / 2.5f64;
    println_f64(div_f64);

    let eq: bool = 1.5f32 * 2.0f32 == 3.0f32;
    println_bool(eq);
}
