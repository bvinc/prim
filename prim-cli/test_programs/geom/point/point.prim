mod point

import std.print.println_i64

struct Point {
    value: i32,
}

trait HasValue {
    fn get(point: Point) -> i32;
}

impl HasValue for Point {
    fn get(point: Point) -> i32 {
        point.value
    }
}

fn new_point() -> Point {
    Point { value = 42 }
}

fn announce() {
    println_i64(42);
}
